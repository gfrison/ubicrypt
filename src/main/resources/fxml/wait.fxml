<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" prefHeight="310.0"
            prefWidth="249.0" xmlns="http://javafx.com/javafx/8.0.102-ea"
            fx:controller="ubicrypt.ui.ctrl.WaitController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <center>
        <ProgressIndicator prefHeight="186.0" prefWidth="174.0" BorderPane.alignment="CENTER">
            <opaqueInsets>
                <Insets left="50.0" right="50.0"/>
            </opaqueInsets>
        </ProgressIndicator>
    </center>
    <top>
        <Label fx:id="title" styleClass="title" BorderPane.alignment="CENTER"/>
    </top>
    <bottom>
        <Label text="Please wait..." BorderPane.alignment="CENTER"/>
    </bottom>
    <opaqueInsets>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
    </opaqueInsets>
</BorderPane>
