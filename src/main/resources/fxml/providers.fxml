<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" stylesheets="../main.css"
      xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ubicrypt.ui.ctrl.ProvidersController" fx:id="root">
    <children>
        <ToolBar prefHeight="40.0" prefWidth="200.0">
            <items>
                <Button mnemonicParsing="false" onMouseClicked="#addFile">
                    <graphic>
                        <ImageView fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/archive-folder.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Add Local Folder"/>
                    </tooltip>
                </Button>
                <Button mnemonicParsing="false">
                    <graphic>
                        <ImageView fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/Amazon-48.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                    <tooltip>
                        <Tooltip text="Add Amazon S3"/>
                    </tooltip>
                </Button>
            </items>
        </ToolBar>
        <ScrollPane VBox.vgrow="ALWAYS">
            <content>
                <ListView prefHeight="340.0" prefWidth="580.0">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="One"/>
                            <String fx:value="Two"/>
                            <String fx:value="Three"/>
                            <String fx:value="Four"/>
                            <String fx:value="Five"/>
                        </FXCollections>
                    </items>
                    <!--
                                        <childrenUnmodifiable>
                                            <ListCell text="s3://ubicrypt">
                                                <graphic>
                                                    <ImageView fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                                        <image>
                                                            <Image url="@images/Amazon-48.png"/>
                                                        </image>
                                                    </ImageView>
                                                </graphic>
                                            </ListCell>
                                        </childrenUnmodifiable>
                    -->
                </ListView>
            </content>
        </ScrollPane>
    </children>
</VBox>
